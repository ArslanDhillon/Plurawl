{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to open the Photos",
          "cameraPermission": "Allow $(PRODUCT_NAME) to open the camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to open the camera"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.447203522675-prf3ds94an41lq557pj2np42qoqbd629"
        }
      ]
    ],
    "name": "Plurawl",
    "slug": "Plurawl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.e8labs.plurawl",
      "iosClientId": "com.googleusercontent.apps.447203522675-prf3ds94an41lq557pj2np42qoqbd629",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.447203522675-prf3ds94an41lq557pj2np42qoqbd629"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.e8labs.plurawl"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3242fd6c-3000-48f9-958f-938a0948e643"
      }
    }
  }
}
